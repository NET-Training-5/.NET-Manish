@model (int, int)
@{
    (int from, int to) = Model;
}

<h1>Hello There!!!</h1>

<div class="container">
    <div class="row">
        @{
            <h3><u>Multiplication Table from @from to @to:</u></h3>
            @for (int i = from; i <= to; i++)
            {
                <div class="col">
                    @for (byte j = 1; j <= 10; j++)
                    {
                        var mul = i * j;
                        <pre>@i * @j = @mul</pre>
                    }
                </div>

            }


        }
    </div>
</div>
